{
    "name" : "marcel-maqsood/middleware-navigation",
    "description" : "The middleware navigation is a PSR-15 middleware that provides navigation functionality to a Laminas Mezzio application",
    "keywords" : [
        "framework",
        "technikhafen",
        "mazedev",
        "Navigation"
    ],
    "license" : "MIT",
    "homepage" : "https://technikhafen.de",
    "require": {
        "laminas/laminas-servicemanager": "^3.22",
        "mezzio/mezzio-router": "^3.17",
        "knplabs/knp-menu": "^3.5"
    },

    "authors":      [
        {
            "name":     "MazeDEV",
            "homepage": "https://technikhafen.de"
        },
        {
            "name":     "Marcel Maqsood"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "MazeDEV\\NavigationMiddleware\\" : "src/"
        }
    },
    "extra": {
        "zf": {
            "component": "MazeDEV\\NavigationMiddleware",
            "config-provider": "MazeDEV\\NavigationMiddleware\\ConfigProvider"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true
}
